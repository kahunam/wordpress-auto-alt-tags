{
	"name": "kahunam/wordpress-auto-alt-tags",
	"description": "WordPress plugin to automatically generate alt tags for images using AI APIs",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Kahunam"
		}
	],
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"wp-coding-standards/wpcs": "^3.0",
		"dealerdirect/phpcodesniffer-composer-installer": "^1.0",
		"phpcompatibility/phpcompatibility-wp": "^2.1"
	},
	"autoload-dev": {
		"psr-4": {
			"AutoAltTags\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "phpunit",
		"test:coverage": "phpunit --coverage-html tests/coverage",
		"phpcs": "phpcs --standard=WordPress .",
		"phpcbf": "phpcbf --standard=WordPress ."
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
